local flowerThingTargets
local sellableZoneTarget

local function getFlowerThingTargets()
    local hasValidTargets = false
    if flowerThingTargets and #flowerThingTargets > 0 then
        hasValidTargets = true
        for _, part in ipairs(flowerThingTargets) do
            if not (part and part.Parent) then
                hasValidTargets = false
                break
            end
        end
    end

    if not hasValidTargets then
        flowerThingTargets = {}

        local gameplay = workspace:FindFirstChild("Gameplay")
        local plots = gameplay and gameplay:FindFirstChild("Plots")
        local specialPlot = plot or (plots and plots:FindFirstChild(tostring(plr.UserId)))
        local plotDecor = specialPlot and specialPlot:FindFirstChild("PlotDecor")
        local flowerGarden = plotDecor and plotDecor:FindFirstChild("FlowerGarden")

        if flowerGarden then
            for _, descendant in ipairs(flowerGarden:GetDescendants()) do
                if descendant.Name == "FlowerThing" and descendant:IsA("BasePart") then
                    table.insert(flowerThingTargets, descendant)
                end
            end
        end
    end

    return flowerThingTargets or {}
end

local function getSellableZoneTarget()
    if sellableZoneTarget and sellableZoneTarget.Parent then
        return sellableZoneTarget
    end

    local gameplay = workspace:FindFirstChild("Gameplay")
    local plots = gameplay and gameplay:FindFirstChild("Plots")
    local targetPlot = plot or (plots and plots:FindFirstChild(tostring(plr.UserId)))
    local plotLogic = targetPlot and targetPlot:FindFirstChild("PlotLogic")
    local zones = plotLogic and plotLogic:FindFirstChild("Zones")

    sellableZoneTarget = zones and zones:FindFirstChild("SellableZone") or nil
    return sellableZoneTarget
end

local function getItemPosition(item)
    if not item then
        return nil
    end

    if item:IsA("Model") then
        local ok, pivot = pcall(function()
            return item:GetPivot()
        end)
        if ok and pivot then
            return pivot.Position
        end

        local primary = item.PrimaryPart or item:FindFirstChildWhichIsA("BasePart")
        if primary then
            return primary.Position
        end
    elseif item:IsA("BasePart") then
        return item.Position
    end

    return nil
end

local function isPointInsidePart(part, position)
    if not (part and part:IsA("BasePart") and position) then
        return false
    end

    local relative = part.CFrame:PointToObjectSpace(position)
    local halfSize = part.Size * 0.5

    return
        math.abs(relative.X) <= halfSize.X and
        math.abs(relative.Y) <= halfSize.Y and
        math.abs(relative.Z) <= halfSize.Z
end

local function isWhitelistedFlowerItem(item)
    local itemPosition = getItemPosition(item)
    if not itemPosition then
        return false
    end

    for _, flowerPart in ipairs(getFlowerThingTargets()) do
        if isPointInsidePart(flowerPart, itemPosition) then
            return true
        end
    end

    if isPointInsidePart(getSellableZoneTarget(), itemPosition) then
        return true
    end

    return false
end

local function tryPickupContainerItem(item)
    if not item or typeof(item.Name) ~= "string" then
        return
    end

    if string.sub(item.Name, 1, 5) ~= "ITEM_" then
        return
    end

    if isWhitelistedFlowerItem(item) then
        return
    end

    remote:FireServer(buffer.fromstring("\v"), buffer.fromstring("\254\1\0\6)" .. item.Name))
end

Container:CreateToggle({
    Name = "Container Item Pickup",
    CurrentValue = false,
    Callback = function(Value)
        if not containerHolder then
            error("watafak no container folder")
            return
        end

        _G.containeritems = Value

        while _G.containeritems do
            for _, container in ipairs(containerHolder:GetChildren()) do
                for _, item in ipairs(container:GetDescendants()) do
                    tryPickupContainerItem(item)
                end
            end

            if itemCache then
                for _, item in ipairs(itemCache:GetChildren()) do
                    tryPickupContainerItem(item)
                end
            end

            for _, cacheName in ipairs(containerCacheNames) do
                local cache = plot.PlotLogic:FindFirstChild(cacheName)
                if cache then
                    for _, item in ipairs(cache:GetChildren()) do
                        tryPickupContainerItem(item)
                    end
                end
            end

            task.wait(0.15)
        end
    end,
})

