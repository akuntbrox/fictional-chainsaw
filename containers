local autoCollectEnabled = false
local autoCollectConnection
local autoCollectToggle

local function isItemCollected(item)
    if not item then
        return true
    end

    local data = PlayerDataController and PlayerDataController.Data
    local attributeName = Constants and Constants.ATTRIBUTE_NAMES and Constants.ATTRIBUTE_NAMES.PLOT_ITEM_UID

    if not data or not attributeName then
        return true
    end

    local uid = item:GetAttribute(attributeName)
    if not uid then
        return true
    end

    local itemEntry = data.Items and data.Items[uid]
    if not itemEntry then
        return true
    end

    return itemEntry.Collected
end

local function isInventoryFull()
    local data = PlayerDataController and PlayerDataController.Data
    if not data then
        warn("Couldnt find profile for inventory check")
        return true
    end

    local current = 0
    for _, entry in data.Items do
        if entry.PickedUp and entry.Collected and not entry.InVault then
            current += 1
        end
    end

    local upgradeSet = DataSets and DataSets.UpgradeDataSet
    local upgradeIdentifier = upgradeSet
        and upgradeSet.Upgrades
        and upgradeSet.Upgrades.MaxInventoryItems
        and upgradeSet.Upgrades.MaxInventoryItems.Identifier

    if not upgradeSet or not upgradeIdentifier then
        warn("Auto collect could not resolve MaxInventoryItems upgrade")
        return true
    end

    local maxAmount = upgradeSet:GetAmountOfUpgradeValueFromTier(data, upgradeIdentifier)
    if not maxAmount then
        warn("Auto collect could not read maximum inventory amount")
        return true
    end

    return current >= maxAmount
end

local function getUncollectedItems()
    local plot = PlotController and PlotController.GetClientPlot and PlotController.GetClientPlot()
    if not plot then
        return nil
    end

    local logic = plot:FindFirstChild("PlotLogic")
    if not logic then
        return nil
    end

    local cache = logic:FindFirstChild("ItemCache")
    if not cache then
        return nil
    end

    local items = {}
    for _, item in ipairs(cache:GetChildren()) do
        if not isItemCollected(item) then
            table.insert(items, item)
        end
    end

    return items
end

local collectItemsInRange

local function setAutoCollectState(value)
    if autoCollectEnabled == value then
        if not value and autoCollectConnection then
            autoCollectConnection:Disconnect()
            autoCollectConnection = nil
        end
        return
    end

    autoCollectEnabled = value

    if value then
        if not autoCollectConnection then
            autoCollectConnection = RunService.RenderStepped:Connect(function()
                if autoCollectEnabled and collectItemsInRange then
                    collectItemsInRange()
                end
            end)
        end
    elseif autoCollectConnection then
        autoCollectConnection:Disconnect()
        autoCollectConnection = nil
    end
end

local function stopAutoCollect()
    setAutoCollectState(false)
    if autoCollectToggle and typeof(autoCollectToggle.Set) == "function" then
        autoCollectToggle:Set(false)
    end
end

collectItemsInRange = function()
    local items = getUncollectedItems()
    if not items or #items == 0 then
        return
    end

    if isInventoryFull() then
        NotificationController.CreateTextNotification({
            Text = "Inventory full. Auto collect disabled";
            Color = Color3.fromRGB(255, 0, 4);
            Duration = 5;
            Sound = 14299265233;
        })
        stopAutoCollect()
        return
    end

    local attributeName = Constants and Constants.ATTRIBUTE_NAMES and Constants.ATTRIBUTE_NAMES.PLOT_ITEM_UID
    local baseDistance = Constants and Constants.ITEMS and Constants.ITEMS.STANDARD_PICKUP_PROX_DISTANCE or 12

    if not attributeName then
        return
    end

    for _, item in ipairs(items) do
        local uid = item:GetAttribute(attributeName)
        if uid then
            local pivotCFrame
            local ok, pivot = pcall(function()
                return item:GetPivot()
            end)
            if ok then
                pivotCFrame = pivot
            elseif item:IsA("BasePart") then
                pivotCFrame = item.CFrame
            end

            if pivotCFrame then
                local data = PlayerDataController and PlayerDataController.Data
                local entry = data and data.Items and data.Items[uid]
                local maxDistance = baseDistance * ((entry and entry.Scale) or 1)
                local success, distance = pcall(function()
                    return plr:DistanceFromCharacter(pivotCFrame.Position)
                end)

                if success and distance < maxDistance then
                    Remotes.Client.ITEM_REMOTES.TOSERVER_PICKUP_ITEM:Fire(true, uid)
                end
            end
        end
    end
end

